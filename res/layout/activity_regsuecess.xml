<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_background"
    tools:context=".RegsuecessActivity" >
    <include layout="@layout/header_view" android:id="@+id/headerView"/>


    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/headerView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/x44"
        android:textSize="@dimen/x28"
        android:text=" " />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x42"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="@dimen/x53"
        android:background="@drawable/btn_selector"
        android:text="@string/wifilist_configuration"
        android:textColor="#ffffff"
        android:layout_marginLeft="@dimen/x10"
        android:layout_marginRight="@dimen/x10"
        android:textSize="@dimen/x15" />

</RelativeLayout>