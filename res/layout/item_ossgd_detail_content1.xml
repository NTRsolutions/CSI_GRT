<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/y13"
        android:background="@color/oss_gd_yuan2"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="@dimen/x10"
        android:layout_marginRight="@dimen/x10"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="@dimen/y120"
        >
        <!--第一行开始-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
          <TextView
              android:layout_width="wrap_content"
              android:layout_height="match_parent"
              android:text="*"
              android:gravity="center"
              android:textColor="@color/note_bg_white"
              android:textSize="@dimen/size_content_title"
              />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="所属区域"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text=":"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:id="@+id/tvGroupName"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=""
                android:gravity="center_vertical"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                android:layout_marginLeft="@dimen/x5"
                />
        </LinearLayout>
        <!--第一行结束-->
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grid_bg_white"
            />
        <!--第二行开始-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="*"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="申请时间"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <ImageView
                android:layout_width="@dimen/x18"
                android:layout_height="match_parent"
                android:src="@drawable/ossgd_workoader_date_icon_1"
                android:layout_marginLeft="@dimen/x5"
                />
            <TextView
                android:id="@+id/tvApplicationTime"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=""
                android:gravity="center_vertical"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                android:layout_marginLeft="@dimen/x5"
                />
        </LinearLayout>
        <!--第二行结束-->
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grid_bg_white"
            />
        <!--第三行开始-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="*"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="要求到达时间"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <ImageView
                android:layout_width="@dimen/x18"
                android:layout_height="match_parent"
                android:src="@drawable/ossgd_workoader_date_icon_1"
                android:layout_marginLeft="@dimen/x5"
                />
            <TextView
                android:id="@+id/tvDoorTime"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=""
                android:gravity="center_vertical"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                android:layout_marginLeft="@dimen/x5"
                />
        </LinearLayout>
        <!--第三行结束-->
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grid_bg_white"
            />
        <!--第四行开始-->
        <LinearLayout
            android:id="@+id/llAppointment"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/tvNoteContent141"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="*"
                android:gravity="center"
                android:textColor="@color/red"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:id="@+id/tvNoteContent142"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="预约上门时间"
                android:gravity="center"
                android:textColor="@color/content_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <ImageView
                android:id="@+id/ivNoteContent143"
                android:layout_width="@dimen/x18"
                android:layout_height="match_parent"
                android:src="@drawable/ossgd_workoader_date_icon_2"
                android:layout_marginLeft="@dimen/x5"
                />
            <TextView
                android:id="@+id/tvAppointment"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=""
                android:gravity="center_vertical"
                android:textColor="@color/content_bg_white"
                android:textSize="@dimen/size_content_title"
                android:layout_marginLeft="@dimen/x5"
                />
        </LinearLayout>
        <!--第四行结束-->
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grid_bg_white"
            />
        <!--第一行开始-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/tvNoteContent151"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="*"
                android:gravity="center"
                android:textColor="@color/red"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:id="@+id/tvNoteContent152"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="详细地址"
                android:gravity="center"
                android:textColor="@color/content_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text=":"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />
            <EditText
                android:id="@+id/etAddress1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="center_vertical"
                android:textColor="@color/content_bg_white"
                android:textSize="@dimen/size_content"
                android:layout_marginLeft="@dimen/x5"
                />
        </LinearLayout>
        <!--第一行结束-->
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grid_bg_white"
            />
    </LinearLayout>
        <!--第五行开始-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:minHeight="@dimen/y20"
            android:layout_marginTop="@dimen/y5"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/y20"
                android:gravity="center"
                android:text="备注"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="@dimen/y20"
                android:text=":"
                android:gravity="center"
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content_title"
                />

            <TextView
                android:id="@+id/tvRemarks1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/x5"
                android:layout_marginBottom="@dimen/y5"
                android:text=""
                android:textColor="@color/note_bg_white"
                android:textSize="@dimen/size_content" />
        </LinearLayout>
        <!--第五行结束-->
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grid_bg_white"
            />
    <!--第五行开始-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:minHeight="@dimen/y20"
        android:layout_marginTop="@dimen/y5"
        >
        <TextView
            android:id="@+id/tvNoteContent171"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y20"
            android:gravity="center"
            android:text="添加备注"
            android:textColor="@color/content_bg_white"
            android:textSize="@dimen/size_content_title" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y20"
            android:text=":"
            android:gravity="center"
            android:textColor="@color/note_bg_white"
            android:textSize="@dimen/size_content_title"
            />

        <EditText
            android:id="@+id/etRemarks1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/y20"
            android:layout_marginLeft="@dimen/x5"
            android:layout_marginBottom="@dimen/y5"
            android:background="@null"
            android:textColor="@color/content_bg_white"
            android:textColorHint="@color/note_bg_white"
            android:textSize="@dimen/size_content" />
    </LinearLayout>
    <!--第五行结束-->
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/grid_bg_white"
        />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y50"
        >
        <Button
            android:id="@+id/btnReceiver1"
            android:layout_width="@dimen/x150"
            android:layout_height="@dimen/y22"
            android:layout_gravity="center"
            android:background="@drawable/selector_circle_btn_blue"
            android:text="接收"
            android:textColor="@color/content_bg_blue"
            android:textSize="@dimen/size_content_title"
            />
    </FrameLayout>
    </LinearLayout>
</LinearLayout>